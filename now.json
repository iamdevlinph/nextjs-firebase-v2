{
  "version": 2,
  "name": "nextjs-firebase",
  "public": false,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next",
      "config": {
        "maxLambdaSize": "11mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/abc",
      "dest": "/custom-link"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
